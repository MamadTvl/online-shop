ct) => (\r\n                                    <Grid className={classes.productItem} md={4} sm={6} item>\r\n                                        <ProductCard product={product} className={classes.card}/>\r\n                                    </Grid>\r\n                                ))\r\n                        }\r\n                        <Grid item xs={12}>\r\n                            <TablePaginationActions buttonGroupClass={classes.buttonGroup} numPages={numPages}\r\n                                                    page={page} onChange={handleChangePages}/>\r\n                        </Grid>\r\n\r\n                    </Grid>\r\n\r\n                    <Grid item className={classes.categoryXsItem} xs={12}>\r\n                        <div style={{width: '100%'}}>\r\n                            <FilterCategory\r\n                                categories={searchStates.categories}\r\n                                dispatch={dispatch}\r\n                            />\r\n                        </div>\r\n                    </Grid>\r\n\r\n                </Grid>\r\n            </div>\r\n        </ShopLayout>\r\n    )\r\n}\r\n\r\nexport default SearchPage","E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Pages\\Styles\\useProductPageStyle.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Routes\\ProfileRoutes.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Routes\\PrivateRoute.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Routes\\PublicRoute.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\utills\\loginTools.js",[],["552","553"],"E:\\mycodes\\js\\didartshop-website\\src\\utills\\Hooks\\useLogin.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Pages\\LoginPage.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Pages\\Styles\\useLoginPageStyle.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Public\\StyledTextField.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Public\\Title.js",[],"E:\\mycodes\\j