ult] = useState()\r\n    const [loading, setLoading] = useState(true)\r\n    const [, create_box] = useAxios({\r\n        url: '/basket/create_box',\r\n        method: 'POST',\r\n    }, {manual: true})\r\n    const [, create_basket] = useAxios({\r\n        url: '/basket/create_basket',\r\n        method: 'POST',\r\n    }, {manual: true})\r\n\r\n    useEffect(() => {\r\n        async function getResult() {\r\n            try {\r\n                setLoading(true)\r\n                const localStorageCart = JSON.parse(localStorage.getItem('cart'))\r\n                if (localStorageCart) {\r\n                    let response = []\r\n                    for (let i = 0; i < localStorageCart.length; i++) {\r\n                        response.push(await create_box({\r\n                            data: {\r\n                                \"count\": localStorageCart[i].count,\r\n                                \"merchandise\": localStorageCart[i].id,\r\n                                \"size\": localStorageCart[i].size,\r\n                                \"color\": localStorageCart[i].color,\r\n                            }\r\n                        }))\r\n                    }\r\n\r\n                    const boxesId = [], boxes = []\r\n                    for (let i = 0; i < response.length; i++) {\r\n                        boxesId.push(response[i].data.data.id)\r\n                        boxes.push(response[i].data.data)\r\n                    }\r\n                    const basketResponse = await create_basket({\r\n                        data: {\r\n                            \"boxes_list\": boxesId,\r\n                        }\r\n                    })\r\n                    setResult({\r\n                        boxes: boxes,\r\n                        basket: basketResponse.data.data,\r\n                    })\r\n                    setLoading(false)\r\n                }\r\n            } catch (err) {\r\n                setLoading(true)\r\n            }\r\n        }\r\n\r\n        if (fetch)\r\n            getResult().then()\r\n    }, [fetch])\r\n\r\n    return [loading, result]\r\n}\r\n\r\nexport default usePostCart","E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Cart\\FactorCard.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Layouts\\CategoriesList.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Layouts\\SearchDialog.js",["650","651"],"import React from \"react\";\r\nimport {Dialog, SvgIcon} from \"@material-ui/core\";\r\nimport {StyledSearchField} from './StyledSearchField'\r\nimport InputAdornment from \"@material-ui/core/InputAdornment\";\r\nimport {useHeaderStyle} from \"./Styles/useHeaderStyle\";\r\nimport Transition from \"./Styles/Transition\";\r\nimport * as PropTypes from \"prop-types\";\r\n\r\nfunction SearchDialog(props) {\r\n    const classes = useHeaderStyle()\r\n    const {searchInput, setSearchInput, open, setOpen, searchHandler} = props\r\n\r\n    const handleSubmit = (event) => {\r\n        searchHandler(event)\r\n        setOpen(false)\r\n    }\r\n    const handleClose = () => {\r\n        setOpen(false)\r\n    }\r\n\r\n    return (\r\n        <Dialog TransitionComponent={Transition} classes={{paper: classes.dialog}} open={open} onClose={handleClose}\r\n                aria-labelledby=\"form-dialog-title\">\r\n            <form onSubmit={handleSubmit} style={{display: 'flex'}}>\r\n                <StyledSearchField\r\n                    id=\"search-dialog-website\"\r\n                    placeholder={'جستجو'}\r\n                    type=\"search\"\r\n                    value={searchInput}\r\n                    onChange={(event) => setSearchInput(event.target.value)}\r\n                    InputProps={{\r\n                        classes: {\r\n                            input: classes.textField,\r\n                        },\r\n                        startAdornment:\r\n                            <InputAdornment onClick={handleSubmit} style={{marginLeft: 8}} position=\"