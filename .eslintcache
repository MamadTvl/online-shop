۰', productImage, false),\r\n    // createData('کتانی نایک اسموکی Nike Smooky مدل لاین ۲۰۲۰ کد ۱۹۴۸۷', '۲٫۶۵۹٫۰۰۰', productImage, false),\r\n]\r\n\r\nfunction SimilarProducts() {\r\n    const size = useWindowSize()\r\n    const classes = useProductSliderStyles()\r\n\r\n    const setComponent = (products, width) => {\r\n        if (products.length >= 4) {\r\n            return <ProductsSlider products={products}/>\r\n        } else if (products.length === 3) {\r\n\r\n            if (window.innerWidth > 800) {\r\n                return <ProductGrid products={products}/>\r\n            } else {\r\n                return <ProductsSlider products={products}/>\r\n            }\r\n        } else if (products.length === 2) {\r\n            if (window.innerWidth > 500) {\r\n                return <ProductGrid products={products}/>\r\n            } else {\r\n                return <ProductsSlider products={products}/>\r\n            }\r\n        } else if (products.length === 1) {\r\n            return <ProductGrid products={products}/>\r\n        }\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <Title title={'محصولات مرتبط'}/>\r\n            {\r\n                size.width !== undefined && setComponent(products)\r\n            }\r\n\r\n        </>\r\n\r\n    )\r\n}\r\n\r\nexport default SimilarProducts","E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Product\\MobileVersion\\MobileProduct.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\utills\\Hooks\\useLogin.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Pages\\Styles\\useLoginPageStyle.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Public\\Step.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Pages\\Styles\\useSignUpPageStyle.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Layouts\\SocialNetwork.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\SignUp\\ThirdStep.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\SignUp\