tLoading] = useState(true)\r\n    const [, getInfo] = useAxios({\r\n        url: '/user/get_profile',\r\n    })\r\n\r\n    useEffect(() => {\r\n        async function getResult() {\r\n            try {\r\n                setLoading(true)\r\n                const response = await getInfo()\r\n                setResult(response.data.data)\r\n                setLoading(false)\r\n            } catch (err) {\r\n                setLoading(false)\r\n            }\r\n        }\r\n\r\n        if (fetch)\r\n            getResult().then()\r\n\r\n    }, [fetch])\r\n\r\n    return [loading, result]\r\n}\r\n\r\nexport default useUserData","E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Pages\\Styles\\useUserInfoPageStyle.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Profile\\PreviewOrderCard.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Profile\\PreviewAddressCard.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\ForgetPass\\FirstStep.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Public\\StyledRadio.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Cart\\Styles\\useAddressCardStyle.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Cart\\Styles\\useOrderCardStyle.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Profile\\Styles\\usePreviewOrderStyle.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Skeletons\\ProductPageSkeleton.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Skeletons\\MobileProductSkeleton.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\Components\\Skeletons\\DesktopProductSkeleton.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\utills\\Auth.js",[],"E:\\mycodes\\js\\didartshop-website\\src\\Shop\\FetchData\\useStateData.js",["665"],"import {useEffect, useState} from \"react\";\r\nimport {useAxios} from \"../../utills/Hooks/useAxios\";\r\n\r\n\r\nfunction useStateData(fetch) {\r\